#include <stdio.h>
void main()
{

    char a[10]="KOHLI";
    char b[10]="PAWAN";
    char c[10]="ROHIT";
    for(int i=0;i<5;i++)
    {

        printf("  %c",a[i]);
    }
    printf("\n");
    for(int i=0;i<5;i++)
    {
        printf("  %c",b[i]);
    }
    printf("\n");
    for(int i=0;i<5;i++)
    {
        printf("  %c",c[i]);
    }
    printf("\n");
}
