#include<stdio.h>
void main()
{
    char a[10]="kohli";
    char b[10]="rohit";
    char c[10]="vijay";

    for(int i=0;i<5;i++)
    {
        printf(" %c",a[i]);
    }
    printf("\n");
    for(int i=0;i<5;i++)
    {
         printf(" %c",b[i]);
    }
    printf("\n");
    for(int i=0;i<5;i++)
    {
        printf(" %c",c[i]);
    }
}
